{
    "name": "bravecollective/neucore-api",
    "description": "Client library of Neucore API",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "http://openapi-generator.tech",
    "license": "MIT",
    "authors": [
        {
            "name": "OpenAPI-Generator contributors",
            "homepage": "https://openapi-generator.tech"
        }
    ],
    "require": {
